      <Pane prefHeight="66.0" prefWidth="1920.0" style="-fx-background-color: #505050;">
         <children>
            <TextField fx:id="searchTextField" layoutX="74.0" layoutY="19.0" onAction="#searchTextFullImageFieldAction" prefHeight="28.0" prefWidth="353.0" promptText="Search cards here" />
